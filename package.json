{
   "name": "basebot",
   "version": "1.0.0",
   "description": "Dynamic Bot Framework for WhatsApp",
   "main": "main.js",
   "scripts": {
      "lint": "eslint . --fix",
      "format": "prettier --write .",
      "start": "ts-node src/main.ts",
      "create:handler": "ts-node core/scripts/create-handler.command.ts",
      "client:disconnect": "ts-node core/scripts/client-disconnect.command.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "migration:generate": "typeorm-ts-node-commonjs migration:generate -d core/databases/data-source.mysql.ts",
      "migration:run": "typeorm-ts-node-commonjs migration:run -d core/databases/data-source.mysql.ts",
      "localdb:generate": "typeorm-ts-node-commonjs migration:generate -d core/databases/data-source.sqlite.ts",
      "localdb:run": "typeorm-ts-node-commonjs migration:run -d core/databases/data-source.sqlite.ts"
   },
   "keywords": [],
   "author": "Liam Mizrahi <liam@liammizrahi.com>",
   "license": "ISC",
   "dependencies": {
      "@discordjs/opus": "^0.9.0",
      "@google-cloud/speech": "^6.1.0",
      "@types/express-handlebars": "^6.0.0",
      "@types/node": "^20.10.5",
      "@typescript-eslint/eslint-plugin": "^6.15.0",
      "@typescript-eslint/parser": "^6.15.0",
      "axios": "^1.6.2",
      "body-parser": "^1.20.2",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "express-handlebars": "^7.1.2",
      "google-auth-library": "^9.9.0",
      "multer": "^1.4.5-lts.1",
      "multer-s3": "^3.0.1",
      "mysql2": "^3.6.5",
      "pg": "^8.4.0",
      "prettier": "^3.1.1",
      "qrcode": "^1.5.3",
      "reflect-metadata": "^0.1.13",
      "spaces": "^0.0.2",
      "sqlite3": "^5.1.7",
      "telegraf": "^4.16.2",
      "ts-node": "^10.9.2",
      "tsconfig-paths": "^4.2.0",
      "typeorm": "0.3.17",
      "typescript": "^5.3.3",
      "uuid": "^9.0.1",
      "whatsapp-web.js": "^1.23.0",
      "xml2js": "^0.6.2"
   },
   "devDependencies": {
      "@types/body-parser": "^1.19.5",
      "@types/dotenv": "^8.2.0",
      "@types/express": "^4.17.21",
      "@types/node": "^16.11.10",
      "@types/qrcode": "^1.5.5",
      "@types/uuid": "^9.0.7",
      "eslint": "^8.56.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-prettier": "^5.1.1",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   }
}
